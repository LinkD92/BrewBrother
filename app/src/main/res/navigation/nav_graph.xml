<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nag_graph"
    app:startDestination="@id/fragBrewList">

    <fragment
        android:id="@+id/fragBrewList"
        android:name="com.symbol.brewbrother.ui.FragBrewList"
        android:label="frag_brew_list"
        tools:layout="@layout/frag_brew_list" >
        <action
            android:id="@+id/action_fragBrewList_to_fragEditBrewNest"
            app:destination="@id/fragEditBrewNest" >
            <argument
                android:name="brew"
                app:nullable="true"
                app:argType="com.symbol.brewbrother.data.Brew"
                />
        </action>
    </fragment>
    <navigation android:id="@+id/fragEditBrewNest"
        app:startDestination="@id/fragEditBrew">
        <fragment
            android:id="@+id/fragEditBrew"
            android:name="com.symbol.brewbrother.ui.FragEditBrew"
            android:label="frag_edit_brew"
            tools:layout="@layout/frag_edit_brew">
            <argument
                android:name="brew"
                app:argType="com.symbol.brewbrother.data.Brew"
                app:nullable="true" />
        </fragment>
    </navigation>

</navigation>